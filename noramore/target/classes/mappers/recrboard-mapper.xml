<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC
"-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="recrboard">
	
	<resultMap id="resultRecr" type="RecrBoard">
	    <result property="boardId" column="board_id"/>
	    <result property="memberId" column="member_id"/>
	    <result property="categoryId" column="category_id"/>
	    <result property="title" column="title"/>
	    <result property="context" column="context"/>
	    <result property="reportCount" column="report_count"/>
	    <result property="maxRecr" column="max_recr"/>
	    <result property="nowRecr" column="now_recr"/>
	    <result property="RecrStatus" column="recr_status"/>
	    <result property="RecrStartDate" column="recr_start_date" javaType="java.util.Date"/>
	    <result property="RecrEndDate" column="recr_end_date" javaType="java.util.Date"/>
	    <result property="RecrOriginalFilename" column="recr_original_filename"/>
	    <result property="RecrRenameFilename" column="recr_rename_filename"/>
	    <result property="RecrActStartDate" column="recr_act_start_date" javaType="java.util.Date"/>
	    <result property="RecrActEndDate" column="recr_act_end_date" javaType="java.util.Date"/>
	    <result property="RecrLocation" column="recr_location"/>
	    <result property="commentCount" column="comment_count"/>
	    <result property="readCount" column="read_count"/>
	    <result property="registrationDate" column="registration_date" javaType="java.util.Date"/>
	    <result property="lastUpdateDate" column="last_update_date" javaType="java.util.Date"/>
	</resultMap>
	
	<select id="selectRecrBoard" resultMap="resultRecr">
		select * from recr_board 
	</select>

</mapper>
